<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="./css/styl.css">
    <link rel="stylesheet" href="./css/prihlaseni.css">
    <script src="./js/kosik.js"></script>
    <title>Prihlaseni</title>
</head>
<header>
    <nav>
        <img id="logo" src="./obrazek/prasatko.png">
        <ul>
            <li><a href="/#domu">Domů</a></li>
            <li><a href="/#produkty">Produkty</a></li>
            <li><a href="/kosik">Košík</a></li>
            <li><a href="/prihlaseni">Přihlášení</a></li>
        </ul>
    </nav>
    
</header>
 <!-- nahledova sekce -->
 <section id="domu">
    <article class="nadpis-article">
        <span id="nadpis">
            <%=titulek%>
        </span>
    </article>
    <article id="nahled">
        <img id="zelenina" src="./obrazek/zeleninka.png">
    </article>
</section>
<body>
 
    
    <table>
        <thead>
            <tr>
                <th>Jmeno</th>
                <th>Email</th>
                <th>Objednavka</th>
            </tr>
        </thead>
        <tbody>
            <% for(const produkty of data){ %>
                <tr>
                    <td><%= produkty.jmeno %></td>
                    <td><%= produkty.email %></td>
                    <td><%= produkty.produkty %></td>
                </tr>
            <% } %>
        </tbody>
    </table>
</body>
</html>
